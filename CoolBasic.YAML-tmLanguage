- name: keyword.operator
  match: (\+|-|\*|=|\/|\<|\>|\\|\.)
- name: keyword
  match: >
    (?x)
    \b(
    (?i)
    (As|Case|Data|Each|Else|ElseIf|End|EndFunction|EndIf|EndSelect|EndType|EndSearch|Exit|
